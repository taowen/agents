{
  "$schema": "../../node_modules/wrangler/config-schema.json",
  "name": "x402-demo",
  "compatibility_date": "2026-01-28",
  "compatibility_flags": ["nodejs_compat"],
  "main": "src/server.ts",
  "assets": {
    "not_found_handling": "single-page-application",
    "run_worker_first": ["/mcp", "/agents/*"]
  },
  "durable_objects": {
    "bindings": [
      {
        "class_name": "PayMCP",
        "name": "PayMCP"
      },
      {
        "class_name": "PayAgent",
        "name": "PAY_AGENT"
      }
    ]
  },
  "migrations": [
    {
      "new_sqlite_classes": ["PayMCP", "PayAgent"],
      "tag": "v1"
    }
  ]
}
