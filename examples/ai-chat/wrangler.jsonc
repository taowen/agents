{
  "$schema": "../../node_modules/wrangler/config-schema.json",
  "compatibility_date": "2026-01-28",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "not_found_handling": "single-page-application",
    "run_worker_first": ["/agents/*", "/oauth/*", "/auth/*", "/api/*"]
  },
  "durable_objects": {
    "bindings": [
      {
        "class_name": "ChatAgent",
        "name": "ChatAgent"
      }
    ]
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "ai-chat-db",
      "database_id": "615cce83-8d31-4a0e-922c-052bf499c1b1"
    }
  ],
  "kv_namespaces": [
    { "binding": "OTP_KV", "id": "2b36327718ac43e2a809b7112fccc17d" }
  ],
  "r2_buckets": [
    {
      "binding": "R2",
      "bucket_name": "ai-chat-files"
    }
  ],
  "main": "src/server/index.ts",
  "migrations": [
    {
      "new_sqlite_classes": ["ChatAgent"],
      "tag": "v1"
    }
  ],
  "name": "ai-chat-example",
  "routes": [
    {
      "pattern": "ai.connect-screen.com",
      "custom_domain": true
    }
  ],
  "browser": {
    "binding": "MYBROWSER"
  },
  "observability": {
    "enabled": true
  }
}
