{
  "$schema": "../../node_modules/wrangler/config-schema.json",
  "name": "mcp-agent-demo",
  "compatibility_date": "2026-01-28",
  "compatibility_flags": ["nodejs_compat"],
  "main": "src/server.ts",
  "assets": {
    "not_found_handling": "single-page-application",
    "run_worker_first": ["/mcp"]
  },
  "durable_objects": {
    "bindings": [
      {
        "class_name": "MyMCP",
        "name": "MyMCP"
      }
    ]
  },
  "migrations": [
    {
      "new_sqlite_classes": ["MyMCP"],
      "tag": "v1"
    }
  ]
}
