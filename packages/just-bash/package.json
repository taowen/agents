{
  "name": "just-bash",
  "version": "2.10.0",
  "description": "A simulated bash environment with virtual filesystem",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vercel-labs/just-bash.git"
  },
  "homepage": "https://github.com/vercel-labs/just-bash#readme",
  "bugs": {
    "url": "https://github.com/vercel-labs/just-bash/issues"
  },
  "type": "module",
  "main": "dist/bundle/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/bundle/index.js"
    }
  },
  "files": [
    "dist/bundle/",
    "dist/*.d.ts",
    "dist/ast/*.d.ts",
    "dist/commands/**/*.d.ts",
    "dist/fs/**/*.d.ts",
    "dist/interpreter/**/*.d.ts",
    "dist/network/**/*.d.ts",
    "dist/parser/*.d.ts",
    "dist/utils/*.d.ts",
    "README.md",
    "dist/AGENTS.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist && tsc && npm run build:lib && npm run build:clean && sed '1,/^-->/d' AGENTS.npm.md > dist/AGENTS.md",
    "build:clean": "find dist -name '*.test.js' -delete && find dist -name '*.test.d.ts' -delete",
    "build:lib": "esbuild dist/index.js --bundle --splitting --platform=browser --format=esm --minify --outdir=dist/bundle --chunk-names=chunks/[name]-[hash] --external:diff --external:minimatch --external:sprintf-js --external:turndown --external:node:* --define:__BROWSER__=true",
    "prepublishOnly": "npm run validate",
    "validate": "npm run lint && npm run knip && npm run typecheck && npm run build && npm run test:run",
    "typecheck": "tsc --noEmit",
    "lint": "biome check . && npm run lint:banned",
    "lint:banned": "node scripts/check-banned-patterns.js",
    "lint:fix": "biome check --write .",
    "knip": "knip",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:comparison": "vitest run --config vitest.comparison.config.ts",
    "test:comparison:record": "RECORD_FIXTURES=1 vitest run --config vitest.comparison.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:coverage:unit": "vitest run --config vitest.unit.config.ts --coverage"
  },
  "keywords": [],
  "author": "Malte and Claude",
  "license": "Apache-2.0",
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "fast-check": "^3.23.2",
    "@types/ini": "^4.1.1",
    "@types/node": "^25.0.3",
    "@types/papaparse": "^5.5.2",
    "@types/sprintf-js": "^1.1.4",
    "@types/turndown": "^5.0.6",
    "@vitest/coverage-v8": "^4.0.18",
    "esbuild": "^0.27.2",
    "knip": "^5.41.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "diff": "^8.0.2",
    "fast-xml-parser": "^5.3.3",
    "file-type": "^21.2.0",
    "ini": "^6.0.0",
    "minimatch": "^10.1.1",
    "papaparse": "^5.5.3",
    "re2js": "^1.2.1",
    "smol-toml": "^1.6.0",
    "sprintf-js": "^1.1.3",
    "turndown": "^7.2.2",
    "yaml": "^2.8.2"
  }
}
